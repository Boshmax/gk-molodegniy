if not exists (
	select 1 from information_schema.routines 
		where specific_schema = N'$(_SCHEMA)' and specific_name = N'$(_OBJECT)' and routine_type = 'function'
)
exec sp_executesql N'
create function $(_SCHEMA).[$(_OBJECT)]()
returns table
as
return (
	select 0 as c
);
';
